for revisions in Searching:
    print(revisions)
Revision7 = re.compile(r'\sshe\s')
Searching = Revision7.finditer(Essay)
for revisions in Searching:
    print(revisions)
Revision8 = re.compile(r'\sher\s')
Searching = Revision8.finditer(Essay)
for revisions in Searching:
    print(revisions)
