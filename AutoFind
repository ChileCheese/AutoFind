import re

User_File = (input("enter name of file:"))
User_input = open(User_File)
Essay = User_input.read()
print(Essay)
User_input.close()

Revision1 = re.compile(r"'t")
Searching = Revision1.finditer(Essay)
for revisions in Searching:
    print(revisions)
Revision2 = re.compile(r'\s[i|I]\s')
Searching = Revision2.finditer(Essay)
for revisions in Searching:
    print(revisions)
Revision3 = re.compile(r'\sme\s')
Searching = Revision3.finditer(Essay)
for revisions in Searching:
    print(revisions)
Revision4 = re.compile(r'\syou\s')
Searching = Revision4.finditer(Essay)
for revisions in Searching:
    print(revisions)
